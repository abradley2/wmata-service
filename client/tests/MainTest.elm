predictionsValue : Result String D.Value
predictionsValue =
    predictionsJSON
        |> D.decodeString
            (D.map2
                Tuple.pair
                (D.field "value" <| D.list Prediction.decodePrediction)
                (D.field "timestamp" <| D.map floor D.float)
            )
        |> Result.mapError D.errorToString
        |> Result.map
            (\( value, timestamp ) ->
                E.object
                    [ ( "value", E.list Prediction.encodePrediction value )
                    , ( "timestamp", E.int timestamp )
                    ]
            )


stationsResult : Result Http.Error (List Station)
stationsResult =
    stationsJSON
        |> D.decodeString (D.list Station.decodeStation)
        |> Result.mapError (always (Http.BadBody "Could not decode stationsJSON"))


