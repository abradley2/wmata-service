findEffect_ : Effect -> (Effect -> Maybe a) -> Maybe a
findEffect_ eff assertion =
    case eff of
        BatchEffect effs ->
            List.foldl
                (\e found -> Maybe.Extra.or found (findEffect_ e assertion))
                Nothing
                effs

        _ ->
            assertion eff


findEffect : a -> (Effect -> Maybe a) -> Effect -> a
findEffect default assertion eff =
    findEffect_ eff assertion |> Maybe.withDefault default


flagsJSON =
    E.object
        [ ( "seeds", E.list E.int [ 0, 0, 0, 0 ] )
        , ( "now", E.int 0 )
        , ( "dev", E.bool True )
        ]


type alias TestApp =
    { model : Model
    , view : H.Html Msg
    , effect : Effect
    }


initTestApp : TestApp
initTestApp =
    let
        ( model, effect ) =
            init flagsJSON
    in
    TestApp model (view model) effect


updateTestApp : Msg -> TestApp -> TestApp
updateTestApp msg testApp =
    let
        ( nextModel, nextEffect ) =
            update msg testApp.model
    in
    { model = nextModel
    , effect = nextEffect
    , view = view nextModel
    }


queryPath : List Selector -> H.Html a -> Query.Single a
queryPath selectors html =
    List.foldl
        (\selector query -> Query.find [ selector ] query)
        (Query.fromHtml html)
        selectors


userInteraction : List Selector -> ( String, D.Value ) -> TestApp -> Result String TestApp
userInteraction selectors ev testApp =
    queryPath selectors testApp.view
        |> Event.simulate ev
        |> Event.toResult
        |> Result.map (\msg -> update msg testApp.model)
        |> Result.map (\( model, effect ) -> TestApp model (view model) effect)


suite : Test
suite =
    describe "Main.elm"
        [ test "We should fetch stations when the application initializes" <|
            \_ ->
                initTestApp
                    |> .effect
                    |> findEffect (fail "Could not find fetch stations effect")
                        (\e ->
                            case e of
                                FetchStationsEffect ->
                                    Just Expect.pass

                                _ ->
                                    Nothing
                        )
        , test "We show the three closest stations when the user's location is discovered" <|
            \_ ->
                initTestApp
                    |> (\testApp ->
                            findEffect (Result.Err "Could not find fetch stations effect")
                                (\e ->
                                    case e of
                                        FetchStationsEffect ->
                                            Just <| Result.Ok testApp

                                        _ ->
                                            Nothing
                                )
                                testApp.effect
                       )
                    |> Result.map
                        (updateTestApp
                            (ReceivedStations
                                (D.decodeString (D.list Station.decodeStation) stationsJSON
                                    |> Result.mapError (always (Http.BadBody ""))
                                )
                            )
                        )
                    |> Result.map
                        (updateTestApp
                            (ReceivedLocation
                                (E.object
                                    [ ( "type", E.string "Success" )
                                    , ( "value", E.list E.int [ 0, 0, 0 ] )
                                    ]
                                )
                            )
                        )
                    |> Result.andThen
                        (userInteraction
                            [ attribute (A.attribute "data-test-option-idx" "0")
                            ]
                            click
                        )
                    |> Result.map (always pass)
                    |> Result.mapError fail
                    |> Result.Extra.merge
        , test "The user can search for stations by name" <|
            \_ ->
                initTestApp
                    |> (\testApp ->
                            findEffect (Result.Err "Could not find fetch stations effect")
                                (\e ->
                                    case e of
                                        FetchStationsEffect ->
                                            Just <| Result.Ok testApp

                                        _ ->
                                            Nothing
                                )
                                testApp.effect
                       )
                    |> Result.map
                        (updateTestApp
