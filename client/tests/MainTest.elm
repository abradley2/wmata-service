                    |> Result.mapError fail
                    |> Result.Extra.merge
        , test "The user can navigate search results with the keyboard" <|
            \_ ->
                initTestApp
                    |> (\testApp ->
                            findEffect (Result.Err "Could not find fetch stations effect")
