            \_ ->
                let
                    logsError =
                        .effect
                            >> findEffect (fail "Did not log an error")
                                (\eff ->
                                    case eff of
                                        LogErrorEffect _ _ ->
                                            Just pass

                                        _ ->
                                            Nothing
                                )
                in
                initTestApp
                    |> (\testApp ->
                            Expect.all
                                [ updateTestApp (ReceivedStations (Result.Err <| Http.BadBody "bad body")) >> logsError
