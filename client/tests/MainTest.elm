                                    case e of
                                        FetchStationsEffect ->
                                            Just <| Result.Ok testApp

                                        _ ->
                                            Nothing
                                )
                                testApp.effect
                       )
                    |> Result.map
                        (updateTestApp
                            (ReceivedStations
                                (D.decodeString (D.list Station.decodeStation) stationsJSON
                                    |> Result.mapError (always (Http.BadBody ""))
                                )
                            )
                        )
                    |> Result.map2
                        (\predictions testApp ->
                            updateTestApp
                                (ReceivedPredictions predictions)
                                testApp
                        )
                        predictionsValue
                    |> Result.andThen
                        (userInteraction
                            [ attribute (A.attribute "aria-autocomplete" "list")
                            ]
                            focus
                        )
                    |> Result.andThen
                        (userInteraction
                            [ attribute (A.attribute "aria-autocomplete" "list")
                            ]
                            (input "east")
                        )
                    |> Result.map
                        (\testApp ->
                            let
                                prevModel =
                                    testApp.model

                                pressedKeys =
                                    { prevModel
                                        | pressedKeys =
                                            [ Keyboard.ArrowLeft
                                            , Keyboard.ArrowDown
                                            , Keyboard.ArrowDown
                                            , Keyboard.ArrowUp
                                            , Keyboard.Enter
                                            ]
                                    }
                            in
                            { testApp | model = processKeys 0 pressedKeys |> Tuple.first }
                        )
                    |> Result.map (updateTestApp (InputBlurred <| Result.Ok ()))
                    |> Result.map (always pass)
                    |> Result.mapError fail
                    |> Result.Extra.merge
